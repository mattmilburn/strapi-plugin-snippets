{
  "name": "strapi-plugin-snippets",
  "version": "1.3.0",
  "description": "A plugin for Strapi CMS that populates custom snippets into API response data.",
  "license": "MIT",
  "strapi": {
    "kind": "plugin",
    "name": "snippets",
    "displayName": "Snippets",
    "description": "Populates custom snippets into API response data."
  },
  "author": {
    "name": "Matt Milburn",
    "url": "https://github.com/mattmilburn"
  },
  "maintainers": [
    {
      "name": "Matt Milburn",
      "url": "https://github.com/mattmilburn"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "format": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "lint": "eslint --fix-dry-run --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "prepare": "husky",
    "test": "jest --testPathPattern='\\/__tests__\\/.*\\.(?:test|spec)\\.js$' --ci 2>&1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "@strapi/helper-plugin": "^5.0.0-alpha.2",
    "@strapi/utils": "^5.0.0-beta.2",
    "immer": "9.0.21",
    "lodash": "4.17.21",
    "react-redux": "8.1.3"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0-beta.2",
    "prop-types": "^15.8.1",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.3",
    "@babel/eslint-parser": "7.24.1",
    "@babel/preset-react": "7.24.1",
    "@strapi/strapi": "^5.0.0-beta.2",
    "@swc/core": "1.4.8",
    "@swc/jest": "0.2.36",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "lint-staged": "15.2.2",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "react": "^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattmilburn/strapi-plugin-snippets.git"
  },
  "keywords": [
    "strapi",
    "plugin",
    "find replace",
    "find and replace",
    "replace",
    "replacement",
    "shortcode",
    "short code",
    "shortcut",
    "short cut",
    "snippet",
    "subtitute",
    "swap",
    "switch",
    "text"
  ]
}
